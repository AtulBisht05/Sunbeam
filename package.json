{
  "name": "sunbeam_weather_app",
  "description": "Weather app",
  "productName": "Sunbeam-Weather-App",
  "version": "1.0.0",
  "license": "MIT",
  "author": "atul bisht <atul.bisht@nerdapplabs.com>",
  "copyright": "Â© 2018 nerdAppLabs software solution",
  "main": "main.js",
  "build": {
    "appId": "com.nerdAppLabs.sunbeam-weather-app",
    "directories": {
      "output": "app"
    },
    "mac": {
      "category": "public.app-category.weather",
      "target": "dmg",
      "icon": "build/SWA.icns"
    },
    "linux": {
      "target": "AppImage"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "build/SWA.ico"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --dev && electron ./dist/index.html",
    "electron-build": "ng build --prod && electron ./dist/index.html",
    "build-ml":"electron-builder -ml",
    "build-mac": "electron-builder --mac",
    "build-linux": "electron-builder  --linux",
    "build-win": "electron-builder  --win --ia32"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@agm/core": "^1.0.0-beta.3",
    "@angular/animations": "^5.2.11",
    "@angular/cdk": "^5.2.5",
    "@angular/cli": "~1.7.4",
    "@angular/common": "^5.2.11",
    "@angular/compiler": "^5.2.11",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/core": "^5.2.11",
    "@angular/forms": "^5.2.11",
    "@angular/http": "^5.2.11",
    "@angular/language-service": "^5.2.10",
    "@angular/material": "^5.2.5",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@angular/router": "^5.2.11",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.109",
    "angular2-chartjs": "^0.5.1",
    "codelyzer": "^4.3.0",
    "core-js": "^2.5.7",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "moment": "^2.22.1",
    "ngx-progressbar": "^2.1.1",
    "protractor": "^5.3.2",
    "rxjs": "^5.5.11",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "zone.js": "^0.8.26"
  }
}